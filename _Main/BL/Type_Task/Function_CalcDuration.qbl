Function CalcDuration
{
  TextBody:
  [*
    // mho23702 Feb-19-2020 (created)
    quantity := this.Operation().PlanUnit().astype( Order ).Quantity();
    speed := this.Machine().Speed();
    
    value := Duration::Hours( quantity / speed );
    
    this.Duration( value );
  *]
}
